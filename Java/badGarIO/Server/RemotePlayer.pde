class RemotePlayer{
  float playerID;
  float radius;
  float x;
  float y;
  float r;
  float g;
  float b;
  
  RemotePlayer(float id, float rad, float x, float y, float r, float g, float b){
    this.playerID=id;
    this.radius=rad;
    this.x=x;
    this.y=y;
    this.r=r;
    this.g=g;
    this.b=b;
  }
}